<!doctype html>
<html lang="fr" >
	<head >
		<meta charset="UTF-8" >
		<meta name="viewport" content="width=device-width, initial-scale=1.0" >
		<meta http-equiv="X-UA-Compatible" content="ie=edge" >
		<title >SfShop</title >
		{% stylesheets filter="cssrewrite" output="dist/css/all.css"
		"assets/css/screen.css"
		"assets/css/print.css"
		"assets/css/ie.css"
		"assets/css/bootstrap.min.css"
		"assets/css/bootstrap-theme.min.css"
		"assets/css/font-awesome.min.css"
		"assets/css/app.css" %}

		<link rel="stylesheet" href="{{ asset_url }}" />
		{% endstylesheets %}

	</head >
	<body class="container" >
		{% include '@sil21Vitrine/Default/menu.html.twig' %}
		{% set showLeftMenu = showLeftMenu|default('yes') %}

		{% if showLeftMenu == 'yes' %}
			<aside class="col-md-3" >
				{% render ( controller('sil21VitrineBundle:Category:list') ) %}
			</aside >
		{% endif %}
		<section class="{{ showLeftMenu == 'yes'  ? 'col-md-9' : 'col-md-12' }}" >
			{% block content %}
			{% endblock %}
		</section >
		
		{% javascripts filter="?jsqueeze" output="dist/js/app.js"
		"assets/js/jquery-3.1.1.min.js"
		"assets/js/bootstrap.min.js"
		"bundles/fosjsrouting/js/router.js"
		"js/fos_js_routes.js"
		"assets/js/app.js"%}
		<script src="{{ asset_url }}" ></script >
		{% endjavascripts %}
	</body >
</html >